    "lint": "eslint \"src/**/*.{ts,tsx,js}\""
  },
  "dependencies": {
    "drizzle-orm": "^0.32.1",
    "postgres": "^3.4.4",
    "drizzle-kit": "^0.23.1"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "bun": ">=1.0.0 <2"
  }
}
