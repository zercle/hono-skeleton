{
  "name": "@zercle/db",
  "version": "0.1.0",
  "private": false,
  "main": "./src/index.ts",
  "scripts": {
    "migrate": "node --input-type=module -e \"import('./scripts/migrate.js')\" || bun run ./scripts/migrate.ts",
